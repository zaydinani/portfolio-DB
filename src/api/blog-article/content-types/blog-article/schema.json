{
  "kind": "collectionType",
  "collectionName": "blog_articles",
  "info": {
    "singularName": "blog-article",
    "pluralName": "blog-articles",
    "displayName": "blog articles",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "article_title": {
      "type": "string",
      "required": true
    },
    "article_slug": {
      "type": "uid",
      "targetField": "article_title",
      "required": true
    },
    "article_content": {
      "type": "blocks",
      "required": true
    },
    "article_excerpt": {
      "type": "string",
      "required": true
    },
    "article_author": {
      "type": "enumeration",
      "enum": [
        "Zayd Inani"
      ],
      "default": "Zayd Inani",
      "required": true
    },
    "article_cover_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "blog_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-tag.blog-tag",
      "inversedBy": "blog_articles"
    }
  }
}
